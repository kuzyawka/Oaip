#include <stdio.h>
#include <math.h>
int main()
{
	printf("Naidem znachenie algebraicheskogo virazheniya bez sravnitelnix i vichislitelnich operaciy: ");
	int a; int b; float vir;
	printf("\n Vvedem a: ");
	scanf_s("%d", &a);
	printf("\nVvedem b: ");
	scanf_s("%d", &b);
	vir = (float)((a + b) * 5) / 10;
	printf("\nVirazhenie= %f", vir);
	return 0;
}
